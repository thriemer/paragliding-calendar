      "content": {
        "schema": "{\n  \"type\": \"object\",\n  \"properties\": {\n    \"weather\": {\n      \"type\": \"object\",\n      \"properties\": {\n        \"timestamp\": {\n          \"type\": \"string\",\n          \"format\": \"date-time\"\n        },\n        \"temperature\": {\n          \"type\": \"number\"\n        },\n        \"wind_speed_ms\": {\n          \"type\": \"number\"\n        },\n        \"wind_direction\": {\n          \"type\": \"number\"\n        },\n        \"wind_gust_ms\": {\n          \"type\": \"number\"\n        },\n        \"precipitation\": {\n          \"type\": \"number\"\n        },\n        \"cloud_cover\": {\n          \"type\": \"number\"\n        },\n        \"pressure\": {\n          \"type\": \"number\"\n        },\n        \"visibility\": {\n          \"type\": \"number\"\n        },\n        \"description\": {\n          \"type\": \"string\"\n        }\n      }\n    },\n    \"launch\": {\n      \"type\": \"object\",\n      \"properties\": {\n        \"site_type\": {\n          \"type\": \"string\"\n        },\n        \"location\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"latitude\": {\n              \"type\": \"number\"\n            },\n            \"longitude\": {\n              \"type\": \"number\"\n            },\n            \"name\": {\n              \"type\": \"string\"\n            },\n            \"country\": {\n              \"type\": \"string\"\n            }\n          }\n        },\n        \"direction_degrees_start\": {\n          \"type\": \"number\"\n        },\n        \"direction_degrees_stop\": {\n          \"type\": \"number\"\n        },\n        \"elevation\": {\n          \"type\": \"number\"\n        }\n      }\n    }\n  }\n}"
      },

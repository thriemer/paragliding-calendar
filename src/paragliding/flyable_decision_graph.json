    {
      "type": "expressionNode",
      "content": {
        "expressions": [
          {
            "id": "a8d2525c-e2d9-480b-a385-3568bbbfc824",
            "key": "FlyableWindDirection",
            "value": "launch.direction_degrees_start < launch.direction_degrees_stop ? (launch.direction_degrees_start < weather.wind_direction and weather.wind_direction < launch.direction_degrees_stop) : (launch.direction_degrees_start < weather.wind_direction or weather.wind_direction < launch.direction_degrees_stop)"
          },
          {
            "id": "a687023a-6796-4040-92f1-e86f3a58dfd5",
            "key": "FlyableGusts",
            "value": "weather.wind_gust_ms < 40/3.6"
          },
          {
            "id": "ac642251-e862-4c90-b4d5-5bc32c1a0136",
            "key": "FlyableWind",
            "value": "weather.wind_speed_ms < 25/3.6"
          },
          {
            "id": "4f01c9c6-2cdc-4694-9c2e-f6c50cb89458",
            "key": "NoRain",
            "value": "weather.precipitation == 0"
          },
          {
            "id": "450a9cc5-2947-4356-aefe-3294ae2f7439",
            "key": "IsMountainStart",
            "value": "launch.site_type == \"Hang\""
          },
          {
            "id": "640162b7-d9b1-4d3e-988a-0f31ae27dff2",
            "key": "flyable",
            "value": "$.FlyableGusts and $.FlyableWind and $.FlyableWindDirection and $.NoRain and $.IsMountainStart"
          }
        ],
